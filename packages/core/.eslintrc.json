{
  "env": {
    "es2022": true,
    "node": true
  },
  "extends": [
    "prettier"
  ],
  "overrides": [
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2022,
    "sourceType": "module"
  },
  "plugins": [
	  "@typescript-eslint",
		"prettier"
  ],
	"rules": {
		"prettier/prettier": [
			"error",
			{
				"endOfLine": "auto"
			}
		]  
	}
}
